<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cp.data.bean">
	<class name="AdminBean" table="admin">
		<id name="id" type="int">
			<generator class="native" />
		</id>
		<property name="adminName" column="admin_Name" />
		<property name="adminPassword" column="admin_Password" />
		<property name="adminRname" column="admin_Rname" />
		<property name="adminStatus" column="admin_Status" />
		<property name="adminCpaname" column="admin_Cpaname" />
	</class>
</hibernate-mapping>